<!-- EXAMPLE MISSION FILE -->

<?xml version="1.0"?>

<!-- COMMANDS:
        16 = waypoint
        20 = return to launch
        21 = land
        22 = takeoff
        179 = set home
     
     FRAMES:
        0 = global
        1 = local_ned (north, east, down)
        3 = global w/ relative altitude
        4 = local_enu (east, north, up)
-->

<missions>
    <mission id="takeoff">
        <waypoint num="0">
            <frame>3</frame>
            <is_current>True</is_current>
            <autocontinue>False</autocontinue>
            <command>16</command>

            <param1>5.0</param1>
            <param2>0</param2>
            <param3>0</param3>
            <param4>0</param4>
            <latitude>0</latitude>
            <longitude>0</longitude>
            <altitude>5.0</altitude>
        </waypoint>

        <waypoint num="1">
            <frame>3</frame>
            <is_current>False</is_current>
            <autocontinue>False</autocontinue>
            <command>22</command>

            <param1>10</param1>
            <param2>0</param2>
            <param3>0</param3>
            <param4>0</param4>
            <latitude>0</latitude>
            <longitude>0</longitude>
            <altitude>5.0</altitude>
        </waypoint>
    </mission>
</missions>
